<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.music_backend.music.mapper.Schedule_Save_Mapper">

    <!-- 查询分页数据 -->
    <insert id="Schedule_Save" parameterType="map">
        INSERT INTO schedule
        VALUES (#{id}, #{name}, #{date}, #{schedule},#{font},#{size},#{position},#{isbold},#{isitalic},#{color})
    </insert>


</mapper>
